{
  "name": "secureEmailUI",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "show secure indicator",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/mail/u/0/*"
      ],
      "js": [
        "js/main.js"
      ],
      "css": [
        "css/style.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["img/*"],
  "permissions": [
    "https://cysec.jshimizu.dev/*"
  ]
}
